{
  "family": "lognook",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "lognook-api",
      "image": "mtmazurik/lognook:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "nooks",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "lognook/api"
        }
      },
      "memory": 1024,
      "cpu": 256
    }
  ],
  "executionRoleArn": "arn:aws:iam::255373080893:role/ecsTaskExecutionRole",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": ".25 vcpu",
  "memory": "1 gb"
}